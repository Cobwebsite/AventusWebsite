:host {
	av-dynamic-row:not(.header) {

        av-dynamic-col:nth-child(1) {
			text-align: left;
		}
		av-dynamic-col:nth-child(2) {
			text-align: justify;
		}

		av-router-link {
			display: contents;
		}
	}

	av-dynamic-row:not(.header):nth-child(odd) {
		background-color: rgba(0, 0, 0, 0.2);
	}
}
